<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- MvcboardVO 클래스 bean -->
	<bean id="mvcboardVO"
		class="com.kh.springodbcTemolate.vo.MvcboardVO" />

	<!-- MvcboardList 클래스 bean -->
	<bean id="mvcboardList"
		class="com.kh.springodbcTemolate.vo.MvcboardList" />

	<!-- Serivce 클래스의 bean 을 만든다. -->
	<bean id="contentView"
		class="com.kh.springodbcTemolate.service.contentViewService" />
	
	<bean id="insert"
		class="com.kh.springodbcTemolate.service.InsertSerive" />

	<!-- MvcboardDAO 클래스의 bean만든다. -->

	<bean id="mvcboardDAO"
		class="com.kh.springodbcTemolate.dao.MvcboardDAO" />





</beans>
